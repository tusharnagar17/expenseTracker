{
  "expo": {
    "name": "expenseTracker",
    "slug": "expenseTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.codeit.expenseTracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
